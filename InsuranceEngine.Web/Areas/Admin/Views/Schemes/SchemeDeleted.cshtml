
@{
    ViewBag.Title = "SchemeDeleted";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutWithMenuNoTreeView.cshtml";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Scheme Deleted</h3>
    </div>
    <div class="panel-body">
        <p>The scheme has been deleted. Click "ok" to return to the schemes list.</p>
        <div class="form-horizontal">
            <div class="form-group">
                <div class="col-sm-4">
                    &nbsp;
                </div>
                <div class="col-sm-1">
                    @Html.ActionLink("Ok", "Index", null, new { @class = "btn btn-success" })
                </div>
            </div>
        </div>
    </div>
</div>