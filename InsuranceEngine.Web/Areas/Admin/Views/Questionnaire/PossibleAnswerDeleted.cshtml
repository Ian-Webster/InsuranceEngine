@using InsuranceEngine.DTO.Utility.Enums.AdminUIEnums
@{
    ViewBag.Title = "PossibleAnswerDeleted";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutWithMenuNoTreeView.cshtml";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Possible Answer Deleted</h3>
    </div>
    <div class="panel-body">
        <p>The possible answer has been deleted. Click "ok" to return to the page list.</p>
        <div class="form-horizontal">
            <div class="form-group">
                <div class="col-sm-4">
                    &nbsp;
                </div>
                <div class="col-sm-1">
                    @Html.ActionLink("Ok", "ViewQuestion", new { selectedTab = QuestionTabs.PossibleAnswers }, new { @class = "btn btn-success" })
                </div>
            </div>
        </div>
    </div>
</div>
